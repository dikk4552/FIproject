<core:FragmentDefinition
	xmlns="sap.ui.comp.valuehelpdialog"
	xmlns:m="sap.m"
	xmlns:fb="sap.ui.comp.filterbar"
	xmlns:core="sap.ui.core">
	<ValueHelpDialog
		title="계정과목표"
		ok=".onValueHelpOkPress"
		cancel=".onValueHelpCancelPress"
		supportRanges="false"
		key="CoA_number"
		descriptionKey="">
		<filterBar>
			<fb:FilterBar advancedMode="true" search=".onFilterBarSearch" isRunningInValueHelpDialog="true">
				<fb:filterGroupItems>
					<fb:FilterGroupItem
						groupName="__$INTERNAL$"
						name="CoA_number"
						label="계정과목표"
						visibleInFilterBar="true">
						<fb:control>
							<m:Input name="CoA_number" />
						</fb:control>
					</fb:FilterGroupItem>
					<fb:FilterGroupItem
						groupName="__$INTERNAL$"
						name="CoA_name"
						label="내 역"
						visibleInFilterBar="true">
						<fb:control>
							<m:Input name="CoA_name" />
						</fb:control>
					</fb:FilterGroupItem>
				</fb:filterGroupItems>
			</fb:FilterBar>
		</filterBar>
	</ValueHelpDialog>
</core:FragmentDefinition>
