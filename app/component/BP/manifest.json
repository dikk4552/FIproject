{
  "_version": "1.42.0",
  "sap.app": {
    "id": "projectBP",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.8.1",
      "toolsId": "d1a40033-ed7a-4cb8-b161-66f0a52632fc"
    },
    "dataSources": {
      "BPservice":{
        "uri": "/bpservice/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "localUri": "localService/metadata.xml",
          "odataVersion": "4.0"
        }
      },
      "GLservice":{
        "uri": "/glservice/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "localUri": "localService/metadata.xml",
          "odataVersion": "4.0"
        }
      }
      
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": false,
    "dependencies": {
      "minUI5Version": "1.102.1",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.f": {},
        "sap.suite.ui.generic.template": {},
        "sap.ui.comp": {},
        "sap.ui.generic.app": {},
        "sap.ui.table": {},
        "sap.ushell": {}
      }
    },
    "componentUsages": {
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "projectBP.i18n.i18n"
        }
      },
      
      "BP":{
        "dataSource": "BPservice",
        "preload": true,
        "settings": {
          "operationMode" : "Server",
          "earlyRequests" : true,
          "synchronizationMode" : "None",
          "autoExpandSelect" : true
        }
      },
      "GL":{
        "dataSource": "GLservice",
        "preload": true,
        "settings": {
          "operationMode" : "Server",
          "earlyRequests" : true,
          "synchronizationMode" : "None",
          "autoExpandSelect" : true
        }
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "async": true,
        "viewPath": "projectBP.view",
        "controlAggregation": "pages",
        "controlId": "app",
        "clearControlAggregation": false
      },
      "routes": [
        {
          "name": "BPmain",
          "pattern": "",
          "target": {
            "name": "BPmain"
          }
        },
        {
          "name": "BPdetail",
          "pattern": "BPdetail/{bpnumber}",
          "target": [
            "BPdetail"
          ]
        },
        {
          "name": "BPcreate",
          "pattern": "BPcreate/{num}/{category}",
          "target": {
            "name": "BPcreate"
          }
        }
        
      ],
      "targets": {
        "BPmain" : {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": true,
          "viewId": "BPmain",
          "viewName": "BPmain"
        },
        "BPdetail" : {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": true,
          "viewId": "BPdetail",
          "viewName": "BPdetail"
        },
        "BPcreate" : {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": true,
          "viewId": "BPcreate",
          "viewName": "BPcreate"
        }
      }
        
      
    },
    "rootView": {
      "viewName": "projectBP.view.App",
      "type": "XML",
      "async": true,
      "id": "App"
    }
  }
}
