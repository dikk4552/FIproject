<core:FragmentDefinition
	xmlns="sap.ui.comp.valuehelpdialog"
	xmlns:m="sap.m"
	xmlns:fb="sap.ui.comp.filterbar"
	xmlns:core="sap.ui.core">
	<ValueHelpDialog
		title="국가/지역"
		ok=".onValueHelpOkPress"
		cancel=".onValueHelpCancelPress"
		supportRanges="false"
		key="Reg_number"
		descriptionKey="">
		<filterBar>
			<fb:FilterBar advancedMode="true" search=".onFilterBarSearch" isRunningInValueHelpDialog="true">
				<fb:filterGroupItems>
					<fb:FilterGroupItem
						groupName="__$INTERNAL$"
						name="Reg_number"
						label="국가 코드"
						visibleInFilterBar="true">
						<fb:control>
							<m:Input name="Reg_number" />
						</fb:control>
					</fb:FilterGroupItem>
					<fb:FilterGroupItem
						groupName="__$INTERNAL$"
						name="Reg_name"
						label="국가/지역 이름"
						visibleInFilterBar="true">
						<fb:control>
							<m:Input name="Reg_name" />
						</fb:control>
					</fb:FilterGroupItem>
				</fb:filterGroupItems>
			</fb:FilterBar>
		</filterBar>
	</ValueHelpDialog>
</core:FragmentDefinition>
